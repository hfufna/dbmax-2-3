<%
***************************
function colorize(cToken)
***************************
local cOut
do case
case cToken="SOS"
   cOut := "yellow"
case cToken="LAV"
   cOut := "#FFFFC1"
case cToken="SERV"
   cOut := "#FBD2E8"
case cToken="CAN"
   cOut := "#FFB3D1"
case cToken="CON"
   cOut := "#353535"
case cToken="OK"
   cOut := "#333333"
case cToken="PRO"
   cOut := "orange"
otherwise
   cOut := "#FFFFFF"
endcase
return cOut

%>
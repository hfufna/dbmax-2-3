set filter to alltrim(ID_TAV) == alltrim(cTav)
go top
rrr := 0
go top
do while not eof()
   rrr++
   cThisColor := colorize(STAT_COM)
   lShow := .t.
   if not xcShowAll
      if STAT_COM="SERV"
         lShow :=.f.
      endif
   endif
   %>
   <!--------- ** ------- //-->
   <% if lShow %>
      <tr>
      <td bgcolor="<%=cThisColor%>"><font size="2" color="red"><%=zz(rrr)%></font></td>

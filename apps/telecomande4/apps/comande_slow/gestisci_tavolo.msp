         <%
      endcase
      %>
      </td>
      <td bgcolor="<%=cThisColor%>">
      <% if STAT_COM="SOSPESA" %>
         <a href="<%=cSelf%>?VAR_ACTION=ANNROW&VAR_ROW=<%=zz(recno())%>&VAR_TAV=<%=cTav%>">
        <img height="<%=cAltPuls%>" src="../../images/pw9_delrow.gif"></a>
      <% else %>   
         <font color="#000000"><%=left(timeDiff(time(),T_SEND),2)%>m</font>
      <% endif %>
      </td>
      </tr>
   <% endif //-lShow%>
   <!--------- ** ------- //-->
   <%
   nTotConto+= PRZ_UN
